{
	"name": "@dxheroes/ado-monorepo",
	"version": "0.1.0",
	"description": "Agentic Development Orchestrator - Multi-agent AI coding orchestration",
	"author": "DX Heroes",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=9.0.0"
	},
	"packageManager": "pnpm@10.22.0",
	"pnpm": {
		"onlyBuiltDependencies": ["@biomejs/biome", "better-sqlite3", "esbuild"]
	},
	"scripts": {
		"build": "pnpm -r build",
		"dev": "pnpm -r --parallel dev",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "pnpm -r typecheck",
		"clean": "pnpm -r clean && rm -rf node_modules"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^22.10.0",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vitest": "^2.1.6"
	},
	"dependencies": {
		"zod": "^4.1.13"
	}
}
