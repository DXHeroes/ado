{
	"name": "@dxheroes/ado",
	"version": "0.1.1",
	"description": "Agentic Development Orchestrator CLI",
	"type": "module",
	"keywords": [
		"ado",
		"cli",
		"ai-agents",
		"orchestrator",
		"development",
		"automation"
	],
	"bin": {
		"ado": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"@dxheroes/ado-adapters": "workspace:*",
		"@dxheroes/ado-api": "workspace:*",
		"@dxheroes/ado-core": "workspace:*",
		"@dxheroes/ado-shared": "workspace:*",
		"@clack/prompts": "^0.9.1",
		"commander": "^13.0.0",
		"picocolors": "^1.1.1",
		"yaml": "^2.6.1"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
