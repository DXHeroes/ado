{
	"name": "@dxheroes/ado-core",
	"version": "0.1.1",
	"description": "Core orchestration engine for ADO",
	"type": "module",
	"keywords": [
		"ado",
		"orchestrator",
		"workflow",
		"ai-agents",
		"routing",
		"rate-limiting"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"@dxheroes/ado-shared": "workspace:*",
		"better-sqlite3": "^11.6.0",
		"pg": "^8.13.1",
		"yaml": "^2.6.1",
		"ioredis": "^5.4.1",
		"nodemailer": "^6.9.16",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/sdk-node": "^0.54.2",
		"@opentelemetry/sdk-metrics": "^1.28.0",
		"@opentelemetry/sdk-trace-node": "^1.28.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.54.2",
		"@opentelemetry/exporter-metrics-otlp-http": "^0.54.2",
		"@opentelemetry/resources": "^1.28.0",
		"@opentelemetry/semantic-conventions": "^1.28.0"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.12",
		"@types/pg": "^8.11.10",
		"@types/nodemailer": "^6.4.16",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vitest": "^2.1.6"
	}
}
