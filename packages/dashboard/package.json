{
	"name": "@dxheroes/ado-dashboard",
	"version": "0.1.2",
	"description": "Web dashboard for ADO orchestrator",
	"type": "module",
	"keywords": [
		"ado",
		"dashboard",
		"ui",
		"react",
		"monitoring"
	],
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:api": "tsc && BUILD_MODE=api vite build",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist node_modules"
	},
	"dependencies": {
		"@dxheroes/ado-core": "workspace:*",
		"@dxheroes/ado-shared": "workspace:*",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.28.0",
		"@tanstack/react-query": "^5.62.0",
		"lucide-react": "^0.462.0",
		"recharts": "^2.14.1",
		"date-fns": "^4.1.0"
	},
	"devDependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.49",
		"tailwindcss": "^3.4.15",
		"vite": "^6.0.1"
	}
}
