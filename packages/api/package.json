{
	"name": "@dxheroes/ado-api",
	"version": "0.1.0",
	"description": "REST API server for ADO orchestrator",
	"type": "module",
	"keywords": ["ado", "api", "rest", "hono", "server"],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"start": "node dist/index.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@dxheroes/ado-core": "workspace:*",
		"@dxheroes/ado-shared": "workspace:*",
		"@hono/node-server": "^1.12.0",
		"hono": "^4.5.0"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"tsup": "^8.0.0",
		"typescript": "^5.4.0"
	}
}
