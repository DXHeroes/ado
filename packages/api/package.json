{
	"name": "@dxheroes/ado-api",
	"version": "0.1.2",
	"description": "REST API server for ADO orchestrator",
	"type": "module",
	"keywords": [
		"ado",
		"api",
		"rest",
		"hono",
		"server"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"start": "node dist/index.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@dxheroes/ado-core": "workspace:*",
		"@dxheroes/ado-shared": "workspace:*",
		"@hono/node-server": "^1.12.0",
		"@trpc/server": "^11.0.0",
		"@trpc/client": "^11.0.0",
		"hono": "^4.5.0",
		"superjson": "^2.2.1",
		"ws": "^8.18.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/ws": "^8.5.12",
		"tsup": "^8.0.0",
		"typescript": "^5.4.0"
	}
}
