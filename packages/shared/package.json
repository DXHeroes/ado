{
	"name": "@dxheroes/ado-shared",
	"version": "0.1.1",
	"description": "Shared types and utilities for ADO",
	"type": "module",
	"keywords": ["ado", "orchestrator", "ai-agents", "typescript", "utilities"],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./test-utils": {
			"types": "./dist/test-utils/index.d.ts",
			"import": "./dist/test-utils/index.js"
		}
	},
	"files": ["dist"],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
