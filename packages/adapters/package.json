{
	"name": "@ado/adapters",
	"version": "0.1.0",
	"description": "Agent adapters for ADO",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"@ado/shared": "workspace:*"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
