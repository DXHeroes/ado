{
	"name": "@dxheroes/ado-adapters",
	"version": "0.1.1",
	"description": "Agent adapters for ADO",
	"type": "module",
	"keywords": [
		"ado",
		"adapters",
		"ai-agents",
		"claude",
		"gemini",
		"cursor",
		"copilot"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"@dxheroes/ado-shared": "workspace:*",
		"@opentelemetry/api": "^1.9.0"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
